<head>
    <partial name="_MetaTags"></partial>

    <title> نرم افزار CRM @ViewData["Title"]</title>

    <partial name="_HeaderReference"></partial>
    @RenderSection("Custome_css",false)
</head>
<body class="theme-cyan">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="m-t-30"><img class="zmdi-hc-spin" src="assets/images/logo.svg" width="48" height="48" alt="قطب نما"></div>
            <p>لطفا صبر کنید ...</p>
        </div>
    </div>
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>

    <!-- Top Bar -->
    <partial name="_TopBarReference"></partial>

    <!-- Left Sidebar -->
    <partial name="_LeftSideBarReference"></partial>

    <!-- Right Sidebar -->
    <partial name="_RightSideBarReference"></partial>

    <!-- Chat-launcher -->
    <partial name="_ChatLauncher"></partial>

    <!-- Main Content -->
    <section class="content home">
        @RenderSection("block_header" , false)
        <div class="container-fluid">
        @RenderBody()
        </div>
    </section>
     
    
    <!-- Jquery Core Js -->
    <partial name="_FooterReference"></partial>
    <partial name="_SiteMessage"></partial>
    @RenderSection("Custom_js",false)
</body>

